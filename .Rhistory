summarise(total_pm = sum(PLUS_MINUS),
total_fgpct = sum(FGM)/sum(FGA),
total_fg3pct = sum(`FG3M`)/sum(`FG3A`),
win_pct = sum(W)/sum(GP),
ft_pct = sum(FTM)/sum(FTA)
) %>%
arrange(desc(win_pct), total_pm)
